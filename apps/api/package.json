  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "build": "nest build",
    "build:docker": "bunx prisma generate && nest build",
    "db:generate": "bun run db:getschema && bunx prisma generate",
    "db:getschema": "node ../../packages/db/scripts/combine-schemas.js && cp ../../packages/db/dist/schema.prisma prisma/schema.prisma",
    "db:migrate": "cd ../../packages/db && bunx prisma migrate dev && cd ../../apps/api",
    "deploy:trigger-prod": "npx trigger.dev@4.0.6 deploy",
    "dev": "bunx concurrently --kill-others --names \"nest,trigger\" --prefix-colors \"green,blue\" \"bun run dev:nest\" \"bunx trigger.dev@4.0.6 dev\"",
    "dev:nest": "nest start --watch --exec 'node dist/apps/api/src/main.js'",
    "postbuild": "node -e \"require('fs').writeFileSync('dist/main.js', 'require(\\\"./apps/api/src/main.js\\\");', 'utf8')\"",
    "dev:trigger": "bunx trigger.dev@4.0.6 dev",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prebuild": "bun run db:generate",
    "start": "node dist/apps/api/src/main",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/apps/api/src/main",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit"
  }
}
